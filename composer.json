{
    "name": "imper86/allegroapi",
    "description": "library making allegro rest and soap api using easier",
    "type": "library",
    "require": {
        "php": ">=7.1.0",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-soap": "*",
        "guzzlehttp/guzzle": "^6.3",
        "ramsey/uuid": "^3.7",
        "lcobucci/jwt": "^3.2",
        "wsdl2phpgenerator/wsdl2phpgenerator": "^3.4"
    },
    "abandoned": "imper86/allegro-rest-api-sdk",
    "suggest": {
        "imper86/allegro-rest-api-sdk": "Package imper86/allegroapi is no longer maintained - use imper86/allegro-rest-api instead"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "imper",
            "email": "me@imper.info"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Imper86\\AllegroApi\\": "src/Imper86/AllegroApi" }
    }
}
